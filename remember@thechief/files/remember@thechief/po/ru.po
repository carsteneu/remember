# Plugin Configuration Dialog
msgid "Plugin Configuration"
msgstr "Настройка Плагина"

msgid "Configure plugin settings. Click 'Edit' to modify values."
msgstr "Настройте параметры плагина. Нажмите 'Изменить' для изменения значений."

msgid "Edit Settings"
msgstr "Изменить Настройки"

msgid "Allgemeine Informationen"
msgstr "Общая Информация"

msgid "Anzeigename"
msgstr "Отображаемое Имя"

msgid "Beschreibung"
msgstr "Описание"

msgid "Fensterklassen"
msgstr "Классы Окон"

msgid "Fensterklassen-Namen (komma-getrennt)"
msgstr "Имена классов окон (через запятую)"

msgid "Start-Konfiguration"
msgstr "Конфигурация Запуска"

msgid "Ausführbare Dateien"
msgstr "Исполняемые Файлы"

msgid "Programmpfade (komma-getrennt)"
msgstr "Пути программ (через запятую)"

msgid "Standard-Flags"
msgstr "Стандартные Флаги"

msgid "Standard Kommandozeilen-Flags (komma-getrennt)"
msgstr "Стандартные флаги командной строки (через запятую)"

msgid "Bedingte Flags wenn aktiviert"
msgstr "Условные флаги при активации"

msgid "Funktionen"
msgstr "Функции"

msgid "Einzelinstanz"
msgstr "Единственный Экземпляр"

msgid "Automatische Wiederherstellung"
msgstr "Автоматическое Восстановление"

msgid "Timeout (ms)"
msgstr "Тайм-аут (мс)"

msgid "Wartezeit in Millisekunden"
msgstr "Время ожидания в миллисекундах"

