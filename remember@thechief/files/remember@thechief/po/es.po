# Plugin Configuration Dialog
msgid "Plugin Configuration"
msgstr "Configuración de Plugin"

msgid "Configure plugin settings. Click 'Edit' to modify values."
msgstr "Configurar ajustes del plugin. Haz clic en 'Editar' para modificar valores."

msgid "Edit Settings"
msgstr "Editar Ajustes"

msgid "Allgemeine Informationen"
msgstr "Información General"

msgid "Anzeigename"
msgstr "Nombre para Mostrar"

msgid "Beschreibung"
msgstr "Descripción"

msgid "Fensterklassen"
msgstr "Clases de Ventana"

msgid "Fensterklassen-Namen (komma-getrennt)"
msgstr "Nombres de clases de ventana (separados por comas)"

msgid "Start-Konfiguration"
msgstr "Configuración de Inicio"

msgid "Ausführbare Dateien"
msgstr "Archivos Ejecutables"

msgid "Programmpfade (komma-getrennt)"
msgstr "Rutas de programa (separadas por comas)"

msgid "Standard-Flags"
msgstr "Indicadores Predeterminados"

msgid "Standard Kommandozeilen-Flags (komma-getrennt)"
msgstr "Indicadores de línea de comandos predeterminados (separados por comas)"

msgid "Bedingte Flags wenn aktiviert"
msgstr "Indicadores condicionales cuando está activado"

msgid "Funktionen"
msgstr "Funciones"

msgid "Einzelinstanz"
msgstr "Instancia Única"

msgid "Automatische Wiederherstellung"
msgstr "Restauración Automática"

msgid "Timeout (ms)"
msgstr "Tiempo de Espera (ms)"

msgid "Wartezeit in Millisekunden"
msgstr "Tiempo de espera en milisegundos"

msgid "Kulanzzeit (ms)"
msgstr "Período de Gracia (ms)"

msgid "Zusätzliche Wartezeit in Millisekunden"
msgstr "Tiempo de espera adicional en milisegundos"

msgid "Saved"
msgstr "Guardado"

msgid "Plugin configuration saved. Restart Cinnamon to apply changes."
msgstr "Configuración del plugin guardada. Reinicie Cinnamon para aplicar los cambios."

msgid "Failed to save plugin configuration."
msgstr "Error al guardar la configuración del plugin."

msgid "Cancel"
msgstr "Cancelar"

msgid "Save"
msgstr "Guardar"

# Close Window Button (windows.py)
msgid "Close Window"
msgstr "Cerrar ventana"

msgid "Cannot close window"
msgstr "No se puede cerrar la ventana"

msgid "No X11 Window ID available for this window."
msgstr "No hay ID de ventana X11 disponible para esta ventana."

msgid "Close window '{0}'?"
msgstr "¿Cerrar ventana '{0}'?"

msgid "This will gracefully close the window. Unsaved work may be lost."
msgstr "La ventana se cerrará correctamente. El trabajo no guardado puede perderse."

msgid "wmctrl not found"
msgstr "wmctrl no encontrado"

msgid "Please install wmctrl: sudo apt install wmctrl"
msgstr "Por favor instale wmctrl: sudo apt install wmctrl"

msgid "Error closing window"
msgstr "Error al cerrar la ventana"
